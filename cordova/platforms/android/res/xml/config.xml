<?xml version='1.0' encoding='utf-8'?>
<widget id="fergardi.cordova.annyversary" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Annyversary</name>
    <description>
    Annyversary
  </description>
    <author email="fergardi@gmail.com">
    Fergardi
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <splash density="land-ldpi" src="res/screen/drawable-land-ldpi/screen.png" />
    <splash density="land-mdpi" src="res/screen/drawable-land-mdpi/screen.png" />
    <splash density="land-hdpi" src="res/screen/drawable-land-hdpi/screen.png" />
    <splash density="land-xhdpi" src="res/screen/drawable-land-xhdpi/screen.png" />
    <splash density="port-ldpi" src="res/screen/drawable-port-ldpi/screen.png" />
    <splash density="port-mdpi" src="res/screen/drawable-port-mdpi/screen.png" />
    <splash density="port-hdpi" src="res/screen/drawable-port-hdpi/screen.png" />
    <splash density="port-xhdpi" src="res/screen/drawable-port-xhdpi/screen.png" />
    <icon density="ldpi" src="res/icon/mipmap-ldpi/icon.png" />
    <icon density="mdpi" src="res/icon/mipmap-mdpi/icon.png" />
    <icon density="hdpi" src="res/icon/mipmap-hdpi/icon.png" />
    <icon density="xhdpi" src="res/icon/mipmap-xhdpi/icon.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Orientation" value="portrait" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="SplashScreen" value="screen" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
